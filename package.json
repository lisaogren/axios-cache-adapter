{
  "name": "axios-cache-adapter",
  "version": "2.0.0",
  "description": "Caching adapter for axios",
  "homepage": "https://github.com/RasCarlito/axios-cache-adapter",
  "author": {
    "name": "Carl Ogren",
    "email": "rascarlito@gmail.com",
    "url": "https://github.com/RasCarlito"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "main": "dist/cache.js",
  "keywords": [
    "axios",
    "cache",
    "xhr",
    "request"
  ],
  "standard": {
    "ignore": [
      "/dist"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "standard src/**/*.js test/**/*.js | snazzy",
    "lint-fix": "standard --fix src/**/*.js test/**/*.js | snazzy",
    "build": "npm run clean && webpack && NODE_ENV=production webpack",
    "test": "NODE_ENV=test karma start karma.conf.js",
    "watch": "NODE_WATCH=true npm test"
  },
  "repository": "https://github.com/RasCarlito/axios-cache-adapter",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "assert": "^1.4.1",
    "babel-loader": "^8.0.0-beta.2",
    "codecov": "^3.0.0",
    "html-webpack-plugin": "^3.0.6",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-html-reporter": "^0.2.7",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "localforage": "^1.3.1",
    "mocha": "^5.0.4",
    "puppeteer": "^0.12.0",
    "rimraf": "^2.6.2",
    "size-limit": "^0.11.4",
    "snazzy": "^7.0.0",
    "standard": "^11.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.4"
  },
  "size-limit": [
    {
      "path": "./src/index.js"
    }
  ]
}
